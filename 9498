#include <stdio.h>

int main() {
    int s;
    scanf("%d",&s);
    if (s>=90){
        printf("A");
    }
    if ((90>s)&&(s>=80)){
        printf("B");
    }
    if ((80>s)&&(s>=70)){
        printf("C");
    }
    if ((70>s)&&(s>=60)){
        printf("D");
    }
    if (60>s){
        printf("F");
    }

    return 0;
}
