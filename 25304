#include <stdio.h>
int main(void){
    int x;
    int n;
    int a;
    int b;
    scanf("%d",&x);
    scanf("%d",&n);
    for(int i=0; i<n; ++i){
        scanf("%d %d",&a,&b);
        x=x-(a*b);       
    }
    if(x==0){
        printf("Yes");
    }
    else{
        printf("No");
    }
    return(0);
}
