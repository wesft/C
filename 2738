#include<stdio.h>
int main(){
    int n,m,i,j;
    scanf("%d %d",&n, &m);
    int arr1[n][m];
    int arr2[n][m];
    int arr3[n][m];
    for(j=0;j<n;j++){
        for(i=0;i<m;i++){
            scanf("%d",&arr1[j][i]);
        }
    }
    for(j=0;j<n;j++){
        for(i=0;i<m;i++){
            scanf("%d",&arr2[j][i]);
        }
    }
    for(j=0;j<n;j++){
        for(i=0;i<m;i++){
            arr3[j][i]=arr1[j][i]+arr2[j][i];
        }
    }
    for(j=0;j<n;j++){
        for(i=0;i<m;i++){
            printf("%d ", arr3[j][i]);
            
        }
        printf("\n");
    }
    
    return(0);
}
